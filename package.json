{
  "name": "ws-rest",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "homepage": "https://github.com/bluelovers/ws-rest#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-rest/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-rest.git"
  },
  "license": "ISC",
  "author": "",
  "workspaces": [
    "packages/@node-novel/*",
    "packages/@bluelovers/*",
    "packages/@demonovel/*",
    "packages/*"
  ],
  "scripts": {
    "prebuild:lockfile": "npx sync-lockfile .",
    "lerna": "yarn run prebuild:lockfile && yarn run lerna:prepublish && lerna publish --yes --cd-version patch && yarn run ci:cache:list",
    "lerna:prepublish": "lerna run --stream --scope @node-novel/cached-* build && lerna run --stream  --scope @demonovel/cached-data build",
    "ci:build": "yarn run ci:test && yarn run lerna:prepublish",
    "ci:cache:list": "ts-node ./script/ci/test/chk-cache",
    "ci:install": "yarn --frozen-lockfile && yarn global add lerna typescript@next ts-node cross-env",
    "ci:push": "ts-node ./script/ci/git-push",
    "ci:task": "ts-node ./script/task",
    "ci:task:novel-opds-now": "ts-node ./script/task/novel-opds-now.ts",
    "ci:test": "ts-node ./script/ci/test",
    "ncu": "npx yarn-tool ncu -u && yarn-tool ws exec yarn-tool ncu -- -u",
    "sort-package-json": "npx yarn-tool sort && yarn-tool ws exec yarn-tool sort"
  },
  "resolutions": {
    "@types/fs-extra": "*",
    "@types/tough-cookie": "< 4",
    "axios": "< 0.19 && >= 0.18"
  },
  "dependencies": {
    "@bluelovers/fast-glob": "^3.0.4",
    "@ts-type/package-dts": "^1.0.20",
    "cjk-conv": "^1.2.84",
    "fs-extra": "^9.0.1",
    "regexp-cjk": "^3.3.50",
    "upath2": "^3.0.5",
    "workspaces-config": "^1.0.12"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.19",
    "@node-novel/md-loader": "^1.0.9",
    "@node-novel/site-cache-util": "^1.0.28",
    "@types/bluebird": "^3.5.32",
    "@types/chai": "^4.2.11",
    "@types/cheerio": "^0.22.18",
    "@types/dotenv": "^8.2.0",
    "@types/env-ci": "^3.1.0",
    "@types/fs-extra": "^9.0.1",
    "@types/hash-sum": "^1.0.0",
    "@types/jest": "^26.0.0",
    "@types/jquery": "^3.3.38",
    "@types/jsdom": "^16.2.3",
    "@types/lodash": "^4.14.155",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.0.13",
    "@types/tough-cookie": "< 4",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^8.0.0",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "cross-ci": "^1.6.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "env-ci": "^5.0.2",
    "filesize": "^6.1.0",
    "jest": "^26.0.1",
    "jest-transform-stealthy-require": "^1.0.0",
    "node-novel-info": "^3.0.5",
    "restful-decorator": "^1.0.42",
    "restful-decorator-plugin-jsdom": "^1.0.22",
    "sync-lockfile": "^1.0.1",
    "ts-jest": "^26.1.0",
    "ts-type": "^1.2.23"
  }
}
