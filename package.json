{
  "name": "ws-rest",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "homepage": "https://github.com/bluelovers/ws-rest#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-rest/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-rest.git"
  },
  "license": "ISC",
  "author": "",
  "workspaces": [
    "packages/@node-novel/*",
    "packages/@bluelovers/*",
    "packages/@demonovel/*",
    "packages/*"
  ],
  "scripts": {
    "prebuild:lockfile": "ynpx sync-lockfile",
    "ci:test": "ts-node ./script/ci/test",
    "ci:install": "yarn --frozen-lockfile && yarn global add lerna typescript@next ts-node cross-env",
    "ci:install:task:cache": "yarn add -W regexp-cjk cjk-conv @novel-segment/util @node-novel/sort node-novel-info",
    "ci:build": "yarn run ci:test && yarn run lerna:prepublish:ci",
    "ci:cache:list": "ts-node ./script/ci/test/chk-cache",
    "ci:push": "ts-node ./script/ci/git-push",
    "ci:task": "ts-node ./script/task",
    "ci:task:novel-opds-now": "ts-node ./script/task/novel-opds-now.ts",
    "lerna": "yarn run lerna:prepublish && lerna publish --yes --bump patch && yarn run ci:cache:list",
    "lerna:prepublish": "lerna run --stream --scope @node-novel/cached-* build && lerna run --stream  --scope @demonovel/cached-data build",
    "lerna:prepublish:ci": "lerna run --stream --concurrency 2 --scope @node-novel/cached-* build --ignore @node-novel/cached-masiro --ignore @node-novel/cached-dmzj && lerna run --stream --concurrency 1 --scope @demonovel/cached-data build",
    "prepare:fix-ws-links": "ynpx @yarn-tool/fix-ws-links",
    "ncu": "npx yarn-tool ncu -u && yarn-tool ws exec yarn-tool ncu -- -u",
    "sort-package-json": "npx yarn-tool sort && yarn-tool ws exec yarn-tool sort"
  },
  "resolutions": {
    "@types/fs-extra": "*",
    "@types/tough-cookie": "< 4",
    "axios": "< 0.19 && >= 0.18"
  },
  "dependencies": {
    "@bluelovers/fast-glob": "^3.0.4",
    "@git-lazy/diff-staged": "^1.0.26",
    "@git-lazy/util": "^1.0.25",
    "@node-novel/sort": "^1.0.17",
    "@novel-segment/util": "^1.0.40",
    "@ts-type/package-dts": "^1.0.45",
    "array-hyper-unique": "^1.1.9",
    "axios": "< 0.19 && >= 0.18",
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.9",
    "cjk-conv": "^1.2.123",
    "cross-fetch": "^3.1.4",
    "dot-values2": "^2.0.3",
    "filesize": "^6.3.0",
    "fs-extra": "^10.0.0",
    "get-value": "^3.0.1",
    "hash-sum": "^2.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-novel-info": "^3.0.13",
    "regexp-cjk": "^3.3.89",
    "regexp-cjk-plugin-escape-unicode-property": "^1.0.21",
    "regexp-cjk-plugin-extra": "^1.0.28",
    "sort-object-keys2": "^2.0.3",
    "str-util": "^2.3.26",
    "string-natural-compare": "^3.0.1",
    "uni-string": "^1.1.10",
    "unset-value": "^2.0.0",
    "upath2": "^3.1.7",
    "uuid": "^8.3.2",
    "workspaces-config": "^1.0.28",
    "zero-width": "^1.0.15"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.20",
    "@node-novel/md-loader": "^1.0.12",
    "@types/bluebird": "^3.5.35",
    "@types/chai": "^4.2.18",
    "@types/cheerio": "^0.22.29",
    "@types/dotenv": "^8.2.0",
    "@types/env-ci": "^3.1.0",
    "@types/fs-extra": "^9.0.11",
    "@types/get-value": "^3.0.1",
    "@types/hash-sum": "^1.0.0",
    "@types/jest": "^26.0.23",
    "@types/jquery": "^3.5.5",
    "@types/jsdom": "^16.2.10",
    "@types/lodash": "^4.14.170",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^15.6.1",
    "@types/tough-cookie": "< 4",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^8.3.0",
    "bluebird-cancellation": "^1.0.14",
    "chai": "^4.3.4",
    "cross-ci": "^1.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "env-ci": "^5.0.2",
    "jest": "^27.0.1",
    "jest-transform-stealthy-require": "^1.0.0",
    "ts-jest": "^27.0.1",
    "ts-type": "^1.2.34"
  }
}
