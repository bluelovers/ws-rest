  "workspaces": [
    "packages/@node-novel/*",
    "packages/@bluelovers/*",
    "packages/*"
  ],
  "scripts": {
    "prebuild:lockfile": "npx sync-lockfile .",
    "lerna": "yarn run prebuild:lockfile && yarn run lerna:prepublish && lerna publish --yes --cd-version patch",
    "lerna:prepublish": "yarn run ci:test && lerna run --scope @node-novel/cached-* build",
    "ncu": "npx npm-check-updates -u",
    "ci:install": "yarn --frozen-lockfile && yarn global add lerna typescript ts-node",
    "ci:test": "ts-node ./script/ci/test",
    "ci:push": "ts-node ./script/ci/git-push",
    "sort-package-json": "npx \"sort-package-json\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@bluelovers/fast-glob": "^3.0.4",
    "@ts-type/package-dts": "^1.0.4",
    "fs-extra": "^8.1.0",
    "upath2": "^2.1.3",
    "workspaces-config": "^1.0.3"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.3",
    "@types/chai": "^4.2.7",
    "@types/fs-extra": "^7.0.0",
    "chai": "^4.2.0",
    "sync-lockfile": "^1.0.1"
  },
  "resolutions": {
    "axios": "< 0.19 && >= 0.18"
  }
